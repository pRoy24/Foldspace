# SamsarOne API key used for text-to-video jobs; generate it from https://app.samsar.one/ and enable API access for your project.
T2V_API_KEY=

# Legacy CDP API key name kept for older tooling; the current Foldspace config expects CDP_API_KEY_ID/CDP_API_KEY_SECRET below.
CDP_API_KEY_NAME=
# Legacy CDP API private key paired with CDP_API_KEY_NAME; leave untouched if other tooling still relies on it.
CDP_API_KEY_PRIVATE_KEY=

# OpenAI project key used by local tooling; manage at https://platform.openai.com/account/api-keys.
OPENAI_API_KEY=

# Optional network hint consumed by the demo UI; defaults to base-sepolia if omitted.
NETWORK_ID=
# Optional, defaults to base-sepolia

# See https://portal.cdp.coinbase.com/products/onchainkit
# Public-facing CDP key used by the demo UI's OnchainKit widgets.
NEXT_PUBLIC_CDP_API_KEY=

# Agentverse bearer token from https://agentverse.ai/ dashboard → API Keys; required for register calls.
AGENTVERSE_API_KEY=

# X402 facilitator API key ID from Coinbase CDP dashboard (Products → X402 → API Keys). Required for authenticated facilitator calls.
CDP_API_KEY_ID=
# X402 facilitator API secret displayed once during key creation; store securely and rotate if exposed.
CDP_API_KEY_SECRET=

# Hex private key for the Base Sepolia wallet that signs settlements and Agentverse challenges (prefix with 0x).
X402_PRIVATE_KEY=
# Default verification/settlement network for X402 operations; use base-sepolia on the Agentverse testnet.
X402_DEFAULT_NETWORK=
# Optional override for a dedicated facilitator URL provided by Coinbase; leave blank to use the public endpoint.
X402_FACILITATOR_URL=

# Override the SamsarOne API base URL if your account is pointed to a bespoke environment; keep default for production.
T2V_BASE_URL=
# Address that should receive video-payment settlements on Base Sepolia (must match an enabled network in Coinbase CDP).
T2V_PAY_TO=
# Asset symbol accepted by the facilitator on Base Sepolia (ensure the asset is enabled in your X402 project).
T2V_ASSET=
# Default network for text-to-video payment requests; align with X402_DEFAULT_NETWORK.
T2V_NETWORK=
# Price per credit in minor units (e.g., 10000 = 0.01 USDC) communicated to the facilitator.
T2V_PRICE_PER_CREDIT_MINOR_UNITS=
# Optional resource identifier echoed in facilitator receipts so you can correlate requests.
T2V_RESOURCE=
# Human-readable description included in payment requirements.
T2V_DESCRIPTION=
# MIME type the facilitator expects for the create payload.
T2V_MIME_TYPE=
# Maximum time in seconds before the facilitator expires unpaid requests.
T2V_MAX_TIMEOUT_SECONDS=

# Optional Agentverse API base; override only if directed to a staging endpoint.
AGENTVERSE_BASE_URL=
# Default agent type to register (mailbox, proxy, or custom). Choose the type provisioned for your Agentverse tenant.
AGENTVERSE_DEFAULT_AGENT_TYPE=
# Registration prefix (agent or test-agent); use test-agent for Base Sepolia deployments.
AGENTVERSE_DEFAULT_PREFIX=
# Callback URL Agentverse should call when dispatching events to your agent.
AGENTVERSE_DEFAULT_ENDPOINT=
